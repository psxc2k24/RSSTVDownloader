#region SitesMenu functions
function CreateSitesMenu {
	$sitesToolStripMenuItem.DropDownItems.Clear()
	[array]$syncHash.AutoTradeSites = @()
	[array]$syncHash.sitesInfo = @()
	$rssStatus.Visible = $false
	foreach ($site in @((Get-Item -Path "$($syncHash.RegBase)\Sites\*").Name | ForEach-Object { ($_.split('\')[-1]) })) {
		$siteProperties = Get-ItemProperty -Path "$($syncHash.RegBase)\Sites\$site"
		$RSSKey = [string]$siteProperties.RSSKey
		$siteMenuText = "$site"
		if ($site -in ($syncHash.ErrorsFound)) {
			$siteMenuText = "$site (Error)"
			$rssStatus.Visible = $true
		}
		$SiteItem = $sitesToolStripMenuItem.DropDownItems.Add($siteMenuText)
		$SiteItem.Tag = "$site"
		$SiteItem.ToolTipText = "Enable/disable the RSS-stream for this site"
		$SiteItem.Add_Click(
			{
				ToggleActivate -siteItem $this
			}
		)
		if ($RSSKey.Length -gt 1 -and $siteProperties.Enabled -eq 1) {
			$SiteItem.Checked = $true
		}
		else {
			$SiteItem.Checked = $false
		}
		$SubSiteItem = $SiteItem.DropDownItems.Add("Set RSSKey")
		$SubSiteItem.Tag = "$site"
		$SubSiteItem.ToolTipText = "Set/change the RSS Key for this site"
		$SubSiteItem.Add_Click(
			{
				$syncHash.CurrentSite = $syncHash.sitesInfo | Where-Object -Property Name -EQ $this.Tag
				#$syncHash.CurrentSite = $this.Tag
				& $rSSKeyToolStripMenuItem_Click
			}
		)
		$SubSiteItem = $SiteItem.DropDownItems.Add("Enable Autotrade for this site")
		$SubSiteItem.Tag = "$site"
		$SubSiteItem.ToolTipText = "Include this site in the autotrade function, ie download the torrent if same release is already downloaded from a different site."
		$SubSiteItem.Add_Click(
			{
				if ($this.Checked -eq $true) {
					$this.Checked = $false
					EnableAutotrade -site $this.Tag -status 0
				}
				else {
					$this.Checked = $true
					EnableAutotrade -site $this.Tag -status 1
				}
			}
		)
		if ($siteProperties.AutoTrade -ne 1) {
			$SubSiteItem.Checked = $false
			[array]$syncHash.AutoTradeSites = $syncHash.AutoTradeSites | ForEach-Object {if ($_ -ne $site) {$_}}
		}
		else {
			$SubSiteItem.Checked = $true
			[array]$syncHash.AutoTradeSites += $site
		}
		$syncHash.sitesInfo += [PSCustomObject]@{
			Name = $site
			Url  = [string]$siteProperties.URL -replace '<RSSKEY>', $siteProperties.RSSKey -replace '<USERID>', $siteProperties.UserID
			CategoryKey = [string]$siteProperties.CategoryKey
			Categories = [array]@([string]$siteProperties.Categories -split ',')
			Enabled = [int]$siteProperties.Enabled
			AutoTrade = [int]$siteProperties.AutoTrade
			RSSKey = [string]$siteProperties.RSSKey
			HelpString = [string]$siteProperties.HelpString
			UserIDRequired = [int]$siteProperties.UserIDRequired
			UserID = [string]$siteProperties.UserID
		}
	}
	Set-ControlTheme -Control $formRSSTVDownloader -Theme $syncHash.ThemeColor
	[array]$syncHash.ErrorsFound = @()
	UpdateSiteDropdown
}

function ToggleActivate {
	param (
		$siteItem
	)
	#$syncHash.CurrentSite = $siteItem.Tag
	$syncHash.CurrentSite = $syncHash.sitesInfo | Where-Object -Property Name -EQ $siteItem.Tag
	if ((Get-ItemPropertyValue -Path "$($syncHash.RegBase)\Sites\$($syncHash.CurrentSite.Name)" -Name "RSSKey" -ErrorAction Stop).Length -le 1) {
		& $rSSKeyToolStripMenuItem_Click
	}
	else {
		if ((Get-ItemPropertyValue -Path "$($syncHash.RegBase)\Sites\$($syncHash.CurrentSite.Name)" -Name "Enabled" -ErrorAction Stop) -ne 1) {
			$siteItem.Checked = $true
			New-ItemProperty -Path "$($syncHash.RegBase)\Sites\$($syncHash.CurrentSite.Name)" -Name "Enabled" -PropertyType DWord -Value 1 -Force -ErrorAction Stop
		}
		else {
			$siteItem.Checked = $false
			New-ItemProperty -Path "$($syncHash.RegBase)\Sites\$($syncHash.CurrentSite.Name)" -Name "Enabled" -PropertyType DWord -Value 0 -Force -ErrorAction Stop
		}
		CreateSitesMenu
	}
}

function EnableAutoTrade {
	param (
		[string]$site = "",
		[int]$status = 0
		)
	$null = New-ItemProperty -Path "$($syncHash.RegBase)\Sites\$site" -Name "AutoTrade" -Value $status -Force -ErrorAction Stop
	if ($status -eq 0) {
		[array]$syncHash.AutoTradeSites = $syncHash.AutoTradeSites | ForEach-Object { if ($_ -ne $site) { $_ } }
	}
	else {
		[array]$syncHash.AutoTradeSites += $site
	}
}
